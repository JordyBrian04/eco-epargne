{
  "expo": {
    "name": "EcoEpargne",
    "slug": "EcoEpargne",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jrdu04.EcoEpargne"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jrdu04.EcoEpargne",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Autoriser ${PRODUCT_NAME} Ã  utiliser FaceID."
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "3d823f28-ad3d-4ac1-b02a-c9992dbb830d"
      }
    },
    "owner": "brian_jordy"
  }
}
